@article{jiangCharacterizationPhotoInducedCharge2014,
  title = {Characterization of {{Photo}}-{{Induced Charge Transfer}} and {{Hot Carrier Relaxation Pathways}} in {{Spinel Cobalt Oxide}} ({{Co}} {\textsubscript{3}} {{O}} {\textsubscript{4}} )},
