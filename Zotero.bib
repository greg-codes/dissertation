@article{chatterleyDissociationDynamicsElectronic2016,
  title = {Dissociation {{Dynamics}} and {{Electronic Structures}} of {{Highly Excited Ferrocenium Ions Studied}} by {{Femtosecond XUV Absorption Spectroscopy}}},
  author = {Chatterley, Adam S. and Lackner, Florian and Pemmaraju, C. D. and Neumark, Daniel M. and Leone, Stephen R. and Gessner, Oliver},
  year = {2016},
