  file = {C\:\\Users\\greg.DESKTOP-HT6OJA0\\Zotero\\storage\\9E9LJ7YZ\\Khan - GENERATION OF SHORT AND INTENSE ATTOSECOND PULSES.pdf},
  language = {en},
