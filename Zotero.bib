@article{vaidaFemtosecondExtremeUltraviolet2016,
  title = {Femtosecond {{Extreme Ultraviolet Photoemission Spectroscopy}}: {{Observation}} of {{Ultrafast Charge Transfer}} at the n-{{TiO}} {\textsubscript{2}} /p-{{Si}}(100) {{Interface}} with {{Controlled TiO}} {\textsubscript{2}} {{Oxygen Vacancies}}},
